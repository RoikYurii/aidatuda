mixin footer()
  +b.FOOTER.footer
    +e.DIV.container
      +e.DIV.contacts-phones
        +b.DIV.footer-contacts-phones
          +e.DIV.text
            +e.P Отдел продаж:
            +e.P Многоканальный:

          +e.DIV.numbers
            +e.P 8920-732-47-12
            +e.P 8920-714-47-12
            +e.P +7(495) 055-4712
    
          +e.I.open.fa.fa-sort-desc.fa-lg(aria-hidden="true")
      +e.DIV.logo
        +b.DIV.logo
          +e.IMG(src='img/logo.png')
      +e.DIV.socials
        +b.DIV.socials
          +e.A.ig(href="")
            i.fa.fa-instagram(aria-hidden="true")
          +e.A.vk(href="")
            i.fa.fa-vk(aria-hidden="true")
          +e.A.fb(href="")
            i.fa.fa-facebook(aria-hidden="true")
          +e.A.ok(href="")
            i.fa.fa-odnoklassniki(aria-hidden="true")
